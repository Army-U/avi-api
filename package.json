{
  "name": "avi-api",
  "version": "0.0.0",
  "description": "The easiest way to upload images, Compatible with numerous cloud storage.",
  "main": "dist/index.js",
  "scripts": {
    "start": "pm2 restart dist/index.js",
    "deploy": "pm2 deploy production update",
    "dev": "nodemon -w src -x ts-node index.ts",
    "build": "rm -rf dist/ && tsc",
    "lint": "tslint --project tsconfig.json",
    "test": "jest"
  },
  "keywords": [
    "figure-bed",
    "figure-bed-tool"
  ],
  "author": "Army-U <wuchengkai0@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@koa/cors": "2.2.1",
    "ali-oss": "5.2.0",
    "aws-sdk": "2.273.1",
    "axios": "0.18.0",
    "co": "4.6.0",
    "config": "1.30.0",
    "cos-nodejs-sdk-v5": "2.4.3",
    "firebase-admin": "5.12.1",
    "flickrapi": "0.6.0",
    "form-data": "2.3.2",
    "grant-koa": "4.1.1",
    "joi": "13.4.0",
    "koa": "2.5.1",
    "koa-body": "4.0.3",
    "koa-logger": "3.2.0",
    "koa-mount": "3.0.0",
    "koa-router": "7.4.0",
    "koa-session": "5.8.1",
    "lodash": "4.17.10",
    "log4js": "2.8.0",
    "no-unhandledrejection": "0.0.0",
    "qiniu": "7.1.7",
    "raven": "2.6.2",
    "sewing": "0.0.49",
    "upyun": "3.3.4",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@types/config": "0.0.34",
    "@types/form-data": "2.2.1",
    "@types/formidable": "1.0.31",
    "@types/koa": "2.0.46",
    "@types/koa-logger": "3.1.0",
    "@types/koa-router": "7.0.28",
    "@types/koa__cors": "2.2.2",
    "@types/lodash": "4.14.110",
    "@types/node": "10.3.4",
    "@types/raven": "2.5.1",
    "@types/uuid": "3.4.3",
    "jasmine-expect": "3.8.3",
    "jest": "23.1.0",
    "nodemon": "1.17.5",
    "pm2": "2.10.4",
    "pngjs-image": "0.11.7",
    "ts-node": "6.1.1",
    "tslint": "5.10.0",
    "tslint-config-standard": "7.0.0",
    "typescript": "2.9.2"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/!(qiniu|ocr).spec.js"
    ],
    "moduleNameMapper": {
      "^services/(.+)": "<rootDir>/dist/src/services/$1"
    },
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "./__tests__/setup.js",
    "unmockedModulePathPatterns": [
      "jasmine-expect"
    ]
  }
}
